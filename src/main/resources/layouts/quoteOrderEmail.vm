  <html>
    <head>
      <title></title>
      
	   <style>
			body {
				font-family: Arial, Helvetica, sans-serif;
			}			
			table {
				font-family: "Lucida Sans Unicode", "Lucida Grande", Sans-Serif;
				font-size: 14px;
				margin: 0px auto;
				margin-top: 10px;
				text-align: left;
				border-collapse: collapse;
				width: 900px;
			}			
			th {
				font-size: 13px;
				font-weight: normal;
				padding: 8px;
				background: #004ba0;	
				color: #fff;
			}			
			td {
				
				background: #fff;
				border-bottom: 1px solid #fff;
				color: black;
				border-top: 1px solid transparent;
				
			}

			.split60{
				width: 60%;
			}
			.split40{
				width: 40%;
			}

			.address1{
				text-align: left;
			    font-style: initial;
			    font-family: serif;
			    font-weight: 300;
			    font-size: 14px;
			    margin-left: 5px;
			}

			.title{
			    font-family: verdana;
			    font-weight: 600;
			    font-size: 16px;
			}

			.code{
  				font-family: verdana;		
			    font-size: 16px;
			    text-align: center;
			}

			.date{
				font-family: verdana;		
			    font-size: 14px;
			    text-align: right;
			    margin-right: 10px;
			}

			#middle_table td{
				border: 1px solid #171717;
			}

			#middle_table th{
				text-align: center;
				border: 1px solid #171717;
			}


			.no_border{
				    border: none !important;
			}

			.ship, .address2, .footer{
			    font-family: verdana;
			}

			.ship{
			    font-weight: 600;
			    font-size: 13px;
			}

			.address2{
			    font-size: 13px;
			}
			
			.total{
				float: right;
			}			

			.footer{
			    font-size: 13px;
			}
			</style>            
    </head>
    
    <body>
			   
		 <table id="">		             		               
			<tr>
				<td colspan="2" class="split60">
					<img src="cid:logo" /> 				
				</td>
				<td colspan="2" class="split40">
					<p class="address1">AERO CONSUMIBLES YORLIB<br>
					Pasaje 16 de Septiembre<br>
					Col. Centro, Toluca Edo de M&eacute;xico C.P. 5000<br>
					Tel. +52 1 722 361 4669
					</p>
				</td>
			</tr>
			<tr>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>
			<tr>
				<td><p class="title">COTIZACI&Oacute;N<p></td>
				<td><p class="code">$quoteOrder.orderNumber<p></td>
				<td><p class="date">$date.format('MMMM dd, yyyy', $currentDate)<p></td>
			</tr>
			<tr>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>
			<tr>
				<td>
					<p>
						$quoteOrder.client.value<br>
						$quoteOrder.client.address<br>
						$quoteOrder.client.state $quoteOrder.client.zip<br>
						$quoteOrder.client.country<br>
					</p>
				</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>

		</table>
			   
         <table id="middle_table">		             		             
             <tr class="borderStrong">
                 <th width="10%">
                 	ITEM
                 </th>
                 <th width="15%">
                 	P / N
                 </th>
                 <th width="20%">
                 	DESCRIPTION
                 </th>
                 <th width="10%">
                 	QTY.
                 </th>
                 <th width="20%">
                 	CONDITION
                 </th>
                 <th width="10%">
                 	PRICE
                 </th>
                 <th width="15%">
                 	TOTAL (MXN)
                 </th>
             </tr>  
             #foreach( $item in $quoteOrder.items )   
	             <tr>            
	               	 <td>
	               	 	&nbsp;&nbsp;$velocityCount&nbsp;&nbsp;
	               	 </td>
	               	 <td>
	               	 	&nbsp;&nbsp;$item.value&nbsp;&nbsp;
	               	 </td>
	                 <td>
	                 	&nbsp;&nbsp;$item.description&nbsp;&nbsp;
	                 </td>
	                 <td align="right">
	                 	&nbsp;&nbsp;$item.qty&nbsp;&nbsp;
	                 </td>
	               	 <td>
	               	 	&nbsp;&nbsp;$item.condition.key&nbsp;&nbsp;
	               	 </td>	 	                 
	                 <td align="right">
						&nbsp;&nbsp;$number.format("currency", $item.costMXN)&nbsp;&nbsp;
	                 </td>     
	                 <td align="right">
	                 	&nbsp;&nbsp;$number.format("currency", $item.subCostMXN)&nbsp;&nbsp;
	                 </td>  
	             </tr>
             #end
			<tr>
				<td class="no_border">&nbsp;</td>
				<td class="no_border">&nbsp;</td>
				<td class="no_border">&nbsp;</td>
				<td class="no_border">&nbsp;</td>
				<td class="no_border">&nbsp;</td>
				<td colspan ="2" class="no_border">
					<p class="total">&nbsp;&nbsp;TOTAL (MXN) = &nbsp;&nbsp;
					&nbsp;&nbsp;$number.format("currency", $quoteOrder.totalMXN)&nbsp;&nbsp;</p>
				</td>				
			</tr>							 
		</table>  
		<table id="bottom_table">
			<tr>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>
			<tr>
				<td colspan="2"><p class="footer">Saludos,</p></td>
				<td colspan="2">&nbsp;</td>
			</tr>
			<tr>
				<td colspan="2">
					<p class="footer">Elia Conde<br>
						Vendedora
					</p>
				</td>
				<td colspan="2">&nbsp;</td>
			</tr>
		</table>   

    </body>
  </html>
